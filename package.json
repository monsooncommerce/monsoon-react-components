{
  "name": "@monsoon_inc/monsoon-components",
  "version": "1.0.0",
  "description": "This is a test",
  "main": "build/index.js",
  "peerDependencies": {
    "react": "^16.1.1"
  },
  "dependencies": {
    "enzyme": "^3.2.0",
    "express": "^4.16.2",
    "immutable": "^3.8.2",
    "jest": "^21.2.1",
    "markdown-loader": "^2.0.1",
    "node-sass": "^4.6.1",
    "prop-types": "^15.6.0",
    "react-hot-loader": "^3.1.3",
    "react-markdown-renderer": "^1.4.0",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.2.0",
    "webpack": "^3.8.1",
    "webpack-merge": "^4.1.1"
  },
  "scripts": {
    "updatepackages": "node wipeandupdate.js && rm -rf node_modules && npm update --save-dev && npm update --save",
    "test": "node scripts/test.js --env=jsdom",
    "start": "webpack-dev-server --env development --config ./config/webpack.config.js",
    "build": "webpack --env production --config ./config/webpack.config.js --watch"
  },
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec|unit))\\.jsx?$",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/test-setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {}
  },
  "author": {
    "name": "monsoon_inc",
    "email": "avontrapp@monsooninc.com"
  },
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "enzyme-adapter-react-15": "^1.0.5",
    "enzyme-adapter-react-16": "^1.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "inquirer": "^5.0.0",
    "postcss-loader": "^2.0.8",
    "raw-loader": "^0.5.1",
    "react": "^16.1.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.2.0",
    "resolve-url-loader": "^2.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack-dev-server": "^2.9.4"
  }
